pipeline{
	agent any
		stages{
			stage('build'){
				steps{
					git "https://github.com/ps442277/DevJava.git"
					echo 'Continuous Build'
				}
			}
			stage('deploy'){
				steps{
					sh "mvn package"
					echo 'Continous Deploy to QA'
				}
			}
			stage('Test'){
				agent{
					docker{image 'ubuntu'}	
				}
				steps{
					echo "Continous Testing"
				}
			}
		}
		post{
			success{
				echo 'success'
			}
			failure{
				echo 'failure'
			}
		}
}	

